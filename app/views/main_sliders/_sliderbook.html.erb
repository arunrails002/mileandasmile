
  <div class="row">
        <div class="col s12 m12 l6">
      <div class="card z-depth-5">
      <div class="carousel carousel-slider center">
        <div class="carousel-fixed-item center">
          <!-- <a class="btn waves-effect purple grey-text darken-text-2">button</a> -->
        </div>
        <div class="carousel-item  red-text" href="#one!">
          <img src="assets/img_1.jpeg">
        </div>
        <div class="carousel-item  red-text" href="#two!">
          <img src="assets/img_5.jpeg">
        </div>
        <div class="carousel-item  red-text" href="#three!">
         <img src="assets/img_7.jpeg">
        </div>
      </div>

      </div>
        </div>
        <div class="col s12 m12 l6">
            <div class="card darken-4 bg-image-1">
              <div class="card-image">

             <!--  <div class="card-content" style="padding: 109px;"> -->
                <span class="card-title font-weight-400 mb-10">Macbook Pro</span>
                <!--   <div class="container"> -->
                    <%= form_for BoardingForm.new, :url => url_for(:controller => 'boarding_forms', :action => 'create') do |f| %>


                        <div class="type">
                        <div class="row">
                                <div class="col s12 m6 l6">
                                  <div class="card purple darken-3">
                                    <div class="card-content white-text">
                                      <span class="card-title">Type</span>
                                          <%= f.text_field :type_value %>
                                          <div class="typebut">
                                          <a class="btn waves-effect waves-light"><i class="material-icons right">send</i>button</a>
                                          </div>
                                    </div>
                                  </div>
                                </div>
                        </div>
                        </div>
                         <div class="spot">
                        <div class="row">
                                <div class="col s12 m6">
                                  <div class="card purple darken-3">
                                    <div class="card-content white-text">
                                      <span class="card-title">Spot</span>            
                                          <%= f.text_field :spot %>
                                              <div class="spotbut">
                        <a class="btn waves-effect waves-light"><i class="material-icons right">send</i>button</a>
                                          </div>
                                      </div>
                                    </div>
                                  </div>
                                </div>
                        </div>
                        <div class="services">
                        <div class="row">
                                <div class="col s12 m6">
                                  <div class="card purple darken-3">
                                    <div class="card-content white-text">   
                                      <span class="card-title">Services</span>         
                                          <%= f.text_field :services %>
                                              <div class="servicesbut">
                        <a class="btn waves-effect waves-light"><i class="material-icons right">send</i>button</a>
                                          </div>
                                      </div>
                                    </div>
                                  </div>
                                </div>
                        </div>
                        <div class="traveldate">
                        <div class="row">
                                <div class="col s12 m6">
                                  <div class="card purple darken-3">
                                    <div class="card-content white-text">
                                      <span class="card-title">Traveldate</span> 
                                          <%= f.date_field :traveldate %>
                                              <div class="travelbut">
                        <a class="btn waves-effect waves-light"><i class="material-icons right">send</i>button</a>
                                          </div>
                                      </div>
                                    </div>
                                  </div>
                                </div>
                        </div>
                         <div class="numberofnights">
                        <div class="row">
                                <div class="col s12 m6">
                                  <div class="card purple darken-3">
                                    <div class="card-content white-text">
                                     <span class="card-title">Numberofnights</span> 
                                          <%= f.number_field :numberofnights %>
                                              <div class="numberofnightsbut">
                        <a class="btn waves-effect waves-light"><i class="material-icons right">send</i>button</a>
                                          </div>
                                      </div>
                                    </div>
                                  </div>
                                </div>
                        </div>
                         <div class="mobile">
                        <div class="row">
                                <div class="col s12 m6">
                                  <div class="card purple darken-3">
                                    <div class="card-content white-text">
                                     <span class="card-title">Mobile</span> 
                                          <%= f.number_field :mobile %>
                                              <div class="mobilebut">
                        <a class="btn waves-effect waves-light"><i class="material-icons right">send</i>button</a>
                                          </div>
                                      </div>
                                    </div>
                                  </div>
                                </div>
                        </div>
                        <div class="email">
                        <div class="row">
                                <div class="col s12 m6">
                                  <div class="card purple darken-3">
                                    <div class="card-content white-text">
                                      <span class="card-title">Email</span> 
                                          <%= f.text_field :email %>
                                              <div class="emailbut">
                                              <button class="btn waves-effect waves-light" type="submit" name="action">Submit
                            <i class="material-icons right">send</i>
                          </button>
                                          </div>
                                      </div>
                                    </div>
                                  </div>
                                </div>
                        </div>
<!-- </div> -->
<% end %>
                 <!--  </div> -->
               </div>
              </div>
            </div>
        </div>
  </div>

<script>
$(document).ready(function(){
// $(".type").hide();
$(".spot").hide();
$(".services").hide();
$(".traveldate").hide();
$(".numberofnights").hide();
$(".mobile").hide();
$(".email").hide();
$(".submit").hide();
    $(".typebut").click(function(){
        $(".spot").show();
        $(".type").hide();
    });
    $(".spotbut").click(function(){
        $(".services").show();
        $(".spot").hide();
    });
    $(".servicesbut").click(function(){
        $(".traveldate").show();
        $(".services").hide();
    });
    $(".travelbut").click(function(){
        $(".numberofnights").show();
        $(".traveldate").hide();
    });
    $(".numberofnightsbut").click(function(){
        $(".mobile").show();
        $(".numberofnights").hide();
    });
    $(".mobilebut").click(function(){
        $(".email").show();
        $(".mobile").hide();
    });
    $(".emailbut").click(function(){
        $(".submit").show();
        $(".email").hide();
    });
    $("#show").click(function(){
        $("p").show();
    });
});
</script>