
<%= render 'contact_us/map' %>
<p id="notice"><%= notice %></p>

<h1>Contact Us</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Phone</th>
      <th>Mobile</th>
      <th>Fax</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_us.each do |contact_u| %>
      <tr>
        <td><%= contact_u.Address %></td>
        <td><%= contact_u.phone %></td>
        <td><%= contact_u.mobile %></td>
        <td><%= contact_u.fax %></td>
        <td><%= contact_u.email %></td>
        <td><%= link_to 'Show', contact_u %></td>
        <td><%= link_to 'Edit', edit_contact_u_path(contact_u) %></td>
        <td><%= link_to 'Destroy', contact_u, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact U', new_contact_u_path %>
